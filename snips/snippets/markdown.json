{
  "eq": {
    "prefix": ["eq"],
    "body": ["", "\\$\\$ $1", "\\$\\$ {#eq-${2: label}}"]
  },
  "Insert Inline Equation": {
    "prefix": "ii",
    "body": ["\\$$1\\$"]
  },
  "Insert Block Equation": {
    "prefix": "$$",
    "body": ["\\$$", "${0}", "\\$$"]
  },
  "img": {
    "prefix": ["img"],
    "body": ["![](./img/$1)"]
  },
  "br": {
    "prefix": ["br"],
    "body": ["<br/>"]
  },
  "todo": {
    "prefix": ["todo"],
    "body": ["* [ ] ${1:task} -- due:${2:today} ${3: project:}"]
  }
}
